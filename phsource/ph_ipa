phoneme n // from ph_hakka
  vcd alv nas
  Vowelout f1=2 f2=1700 -300 250  f3=-100 80  rms=20 brk
  lengthmod 2

  NextVowelStarts
    VowelStart(n/n@)
    VowelStart(n/na)
    VowelStart(n/ne)
    VowelStart(n/ni)
    VowelStart(n/no)
    VowelStart(n/nu)
  EndSwitch

  IF nextPh(isNotVowel) THEN
    length 100
    FMT(n/n_long_, 50)
  ENDIF
  FMT(n/_n)
endphoneme

phoneme N
  vcd vel nas
  ipa ŋ
  lengthmod 6

  Vowelout f1=2 f2=1900 0 250 f3=0 80 len=15 rms=25 colr=1.0

  NextVowelStarts
    VowelStart(nn/nn@)
    VowelStart(nn/nna)
    VowelStart(nn/nne)
    VowelStart(nn/nni)
    VowelStart(nn/nno)
    VowelStart(nn/nnu)
  EndSwitch

  IF prevPh(#i) THEN
    VowelEnding(nn/inn, -20)
  ENDIF

  IF prevPh(isNotVowel) AND nextPhW(isNotVowel) THEN
    lengthmod 1.5
    FMT(nn/nn_, 60)
  ELIF prevPh(isPause) THEN
    FMT(nn/_nn, 70)
  ELIF nextPh(isNotVowel) THEN
    lengthmod 2
    FMT(nn/nn2_, 65)
  ELSE
    FMT(nn/nn_, 65)
  ENDIF
endphoneme

