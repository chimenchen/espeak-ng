phoneme lh  //voiceless
  vls alv frc  // liquid
  ipa l̥
  lengthmod 3

  NextVowelStarts
    VowelStart(l/l@)
    VowelStart(l/la)
    VowelStart(l/le)
    VowelStart(l/li, -10)
    VowelStart(l/lo)
    VowelStart(l/lu)
  EndSwitch

  IF prevPh(isVowel) THEN
    VowelEnding(l/xl, -30)
  ENDIF

  WAV(ufric/l#, 10) addWav(ufric/tlh, 20)
endphoneme

